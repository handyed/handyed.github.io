{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/test",
            "dest": "server.js"
        },
        {
            "src": "/api/estimate-price",
            "dest": "server.js"
        },
        {
            "handle": "filesystem"
        }
    ],
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 