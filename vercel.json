{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 10,
                "memory": 1024
            }
        }
    ],
    "routes": [
        {
            "src": "/api/test",
            "dest": "server.js"
        },
        {
            "src": "/api/estimate-price",
            "dest": "server.js"
        },
        {
            "handle": "filesystem"
        }
    ]
} 